<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./head'); %>
  </head>
  <body>
    <%- include('./header'); %>
    <main>
      <section id="myResources">
        <div>
          <h3>List of my save and liked resources:</h3>
          <% for(const row of rows) { %>
          <div class="imageClass" style= "border: 1px solid black ; padding: 20px">
            <img src="<%= row.image %>" width="100" height="100">
            <p><br/></p>
            <p><b>TITLE:</b><%= row.title %></p>
            <p><b>CATEGORY:</b><%= row.category_name %></p>
            <a href="<%= row.url %>"><%= row.url %></a>
            <button class="likeButton" type="submit">Like</button>
          </div>
          <% } %>
        </div>
      </section>
    </main>
  </body>
</html>
